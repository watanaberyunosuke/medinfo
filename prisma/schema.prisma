generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Term {
  id Int @id @default(autoincrement())
  short_form String
  type String
  full_form String @db.VarChar(255)
  cht String @db.VarChar(255)
  chs String @db.VarChar(255)
  annotation String? @db.VarChar(255)
  resources String? @db.VarChar(255)
}

model AustraliaHospital {
  objectid  Int     @id @default(autoincrement())
  hsib_id   String
  hosp_name String
  category  String
  street    String
  pcode     String
  suburb    String
  state     String
  xcoord    Float
  ycoord    Float
  globalid  String
}


enum type {
  Policy
  Term
  Occupation
  Therapy
  Assessment
}
